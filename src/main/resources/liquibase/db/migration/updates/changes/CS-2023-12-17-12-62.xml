<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog classpath:/liquibase/config/dbchangelog-4.6.xsd">

    <changeSet id="add-foreign-key-prime-delegation" author="A.LAHLIB">
        <addColumn tableName="prime">
            <column name="delegation" type="BIGINT"/>
        </addColumn>
        <addForeignKeyConstraint
                baseTableName="prime"
                baseColumnNames="delegation"
                constraintName="FK_Prime_delagation"
                referencedTableName="entite"
                referencedColumnNames="id"
                onDelete="CASCADE"
                onUpdate="CASCADE"
        />
    </changeSet>


</databaseChangeLog>